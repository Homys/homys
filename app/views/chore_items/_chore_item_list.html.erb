<% @chore_items.each do |chore| %>
  <%= chore.name %> <br>
  Description: <%= chore.description %> <br>
  <% if chore.user %>
    Assigned to: <%= chore.user.full_name %> <br>
  <% end %>
  <%= link_to("Delete", house_chore_item_path(current_user.house_id, chore), method: :delete) if chore.owner_id == current_user.id %> <br><br>
<% end %>